"Start Symbol" = <S>
<S> ::= <S1> <S> | <S1>
<S1> ::= <A> | <B> | <E>
<A> ::= 'id' '=' <D> ';' | 'id' '+=' <D> ';' | 'id' '-=' <D> ';'
<B> ::= <C> 'id' ';' | <C> 'id' '=' <D> ';'
<C> ::= 'var' | 'const'
<D> ::= <D> '+' <D> | <D> '-' <D> | 'id' | '(' <D> ')' | <F>
<E> ::= 'if' '(' <D> ')' '{' <S> '}' | 'if' '(' <D> ')' '{' <S> '}' 'else' '{' <S> '}'
<F> ::= 'numero' | 'true' | 'false' | '"' 'string' '"'
